FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (291. 0 0);

boundaryField
{
    my-velocity-inlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (291. 0 0);
    }

    my-pressure-outlet
    {
        type            zeroGradient;
    }

    my-wall
    {
        type            fixedValue;
        value           uniform (0.0 0.0 0.0);
    }

    "(wedgeplanesFront|wedgeplanesBack)"
    {
        type            wedge;
    }

    freeSurf
    {
        type            symmetryPlane;
    }
}

